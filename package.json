{
  "name": "sanblas",
  "version": "1.0.0",
  "scripts": {
    "app": "concurrently \"yarn:webpack-client\" \"yarn:webpack-build-script\" --prefix-colors=yellow.dim,green.dim,blue.dim",
    "all": "concurrently \"yarn:webpack-client\" \"yarn:webpack-build-script\" \"yarn:storybook\" --prefix-colors=yellow.dim,green.dim,cyan.dim,magenta.dim",
    "webpack-client": "webpack-dev-server --config ./config/webpack.config.client.js",
    "webpack-build-script": "webpack --watch --config ./config/webpack.config.build_script.js",
    "storybook": "start-storybook --ci --port 9001"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.1.10",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "fela": "^10.6.1",
    "fela-dom": "^10.6.1",
    "file-loader": "^4.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fela": "^10.6.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
